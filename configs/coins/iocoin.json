{
  "coin": {
      "name": "iocoin",
      "shortcut": "iocoin",
      "label": "iocoin",
      "alias": "iocoin"
  },
  "ports": {
    "backend_rpc": 43765,
    "backend_message_queue": 43764,
    "blockbook_internal": 9030,
    "blockbook_public": 9130
  },
  "ipc": {
    "rpc_url_template": "http://127.0.0.1:{{.Ports.BackendRPC}}",
    "rpc_user": "rpc",
    "rpc_pass": "rpcpass",
    "rpc_timeout": 25,
    "message_queue_binding_template": "tcp://206.189.115.69:{{.Ports.BackendMessageQueue}}"
  },
  "backend": {
    "package_name": "backend-iocoin",
    "package_revision": "iocoin-1",
    "system_user": "ioc",
    "version": "0.0.1",
    "binary_url": "http://206.189.115.69/dist/iocoin-0.0.1.tar.gz",
    "extract_command": "tar -C backend --strip 1 -xf",
    "exclude_files": [
    ],
    "exec_command_template": "{{.Env.BackendInstallPath}}/{{.Coin.Alias}}/bin/iocoind -datadir={{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend -conf={{.Env.BackendInstallPath}}/{{.Coin.Alias}}/{{.Coin.Alias}}.conf -pid=/run/{{.Coin.Alias}}/{{.Coin.Alias}}.pid",
    "logrotate_files_template": "{{.Env.BackendDataPath}}/{{.Coin.Alias}}/backend/*.log",
    "postinst_script_template": "",
    "service_type": "forking",
    "service_additional_params_template": "",
    "protect_memory": true,
    "mainnet": true,
    "server_config_file": "iocoin.conf",
    "additional_params": {
      "mainchainrpcport": "43765",
      "mainchainrpcuser": "rpc",
      "mainchainrpcpassword": "rpc"
    }
  },
  "blockbook": {
    "package_name": "blockbook-iocoin",
    "system_user": "blockbook-iocoin",
    "internal_binding_template": ":{{.Ports.BlockbookInternal}}",
    "public_binding_template": ":{{.Ports.BlockbookPublic}}",
    "explorer_url": "",
    "additional_params": "-dbcache=1073741824",
    "block_chain": {
      "parse": true,
      "mempool_workers": 8,
      "mempool_sub_workers": 2,
      "block_addresses_to_keep": 300,
      "additional_params": {}
    }
  },
  "meta": {
    "package_maintainer": "dh",
    "package_maintainer_email": ""
  }
}
